<% if user_signed_in? %>
  <%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
<% else %>
  <div class="">
    <%= link_to "Sign In", new_user_session_path %>
    <%= link_to "Sign Up", new_user_registration_path %>
  </div>
<% end %>
<br>
<%= form_tag(search_artist_path) do %>
  <div class="">
    <%= text_field_tag :search_artist, "", placeholder: "Search for an artist" %>
    <%= submit_tag "Go" %>
  </div>
<% end %>
